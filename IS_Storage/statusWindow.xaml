<Window x:Class="IS_Storage.statusWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IS_Storage"
        mc:Ignorable="d"
        Title="statusWindow" ShowInTaskbar="False" Height="120" Width="275" WindowStyle="None" WindowStartupLocation="CenterScreen">
    <TextBlock Margin="10" VerticalAlignment="Center" >
        <Run x:Name="headL" Text="dw"/>
        <Run x:Name="textL" Text="dwd"/>
    </TextBlock>
</Window>
